@model ViewCategoryRequestModel
@{
 Layout="~/Views/Shared/_CustomerLayout.cshtml";
  ViewData["Title"] = "Customer";
}
<h3> <i> Here is the Category!</i></h3>
<div class="card-header" style="background-color:midnightblue;">
    <div class="card-title">
        <h1 class="text-center">Category Found </h1>
    </div>
</div>

<dl class="row">
    <dt class="col-sm-2">
        Name:
    </dt>
    <dd class="col-sm-10">
        @Model.CategoryName
    </dd>
   
    <dt class="col-sm-2">
       Number Of Treats In Category:
    </dt>
    <dd class="col-sm-9">
        @Model.NumberOfTreatsInCategory
    </dd>
   <dt class= "col-sm-2">
       Date Of Creation
   </dt>
   <dd class= "col-sm-10">
       @Model.DateOfCreation
   </dd>

      
</dl>
